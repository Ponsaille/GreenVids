{
  "manifest_version": 2,
 
  "name": "GreenVids",
  "version": "0.1.0",
  "description": "Allows you to automaticly set the quality qualities of internets videos",

  "content_scripts": [
    {
      "js": ["contents/youtube.js"],
      "run_at": "document_start",
      "matches": ["*://www.youtube.com/watch*"]
    },
    {
      "js": ["contents/twitch.js"],
      "run_at": "document_start",
      "matches": ["*://www.twitch.tv/*"]
    },
    {
      "js": ["contents/dailymotion.js"],
      "run_at": "document_start",
      "matches": ["*://www.dailymotion.com/*"]
    }
  ]
 
}